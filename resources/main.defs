<svg>
    <defs>
        <link rel="stylesheet" href="main-style.css" />
        <link rel="import" href="/mnt/sysassets/widgets/text_button.defs" />
        <symbol id="main-screen">
            <rect id="main-screen__rect">
                <use 
                    id="btn-left" 
                    href="#text-button" 
                    class="text-button top left application-fill" />
                <use 
                    id="btn-right" 
                    href="#text-button" 
                    class="text-button top right application-fill" 
                />
                <text
                    id="main-screen__center-date-text" 
                >
                </text>
                <text
                    id="main-screen__center-side-text" 
                >
                </text>
                <use 
                    id="btn-history" 
                    href="#text-button" 
                    class="text-button bottom application-fill" />
            </rect>
        </symbol>
    </defs>
</svg>